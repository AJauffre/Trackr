<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="ehcache.xsd" updateCheck="true" monitoring="autodetect" dynamicConfig="true">

    <cache name="geocodedLocations" maxEntriesLocalHeap="10000"   maxEntriesLocalDisk="1000" diskSpoolBufferSizeMB="20"  eternal="false" timeToIdleSeconds="300" timeToLiveSeconds="10000" memoryStoreEvictionPolicy="LFU" transactionalMode="off">
        <persistence strategy="localTempSwap" />
    </cache>

    <diskStore path="java.io.tmpdir"/>


    <cache name="test" maxEntriesLocalHeap="10000"   maxEntriesLocalDisk="1000" diskSpoolBufferSizeMB="20"  eternal="false" timeToIdleSeconds="300" timeToLiveSeconds="10000" memoryStoreEvictionPolicy="LFU" transactionalMode="off">
        <persistence strategy="localTempSwap" />
    </cache>

</ehcache>